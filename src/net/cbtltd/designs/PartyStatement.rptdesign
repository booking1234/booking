            <xml-property name="queryText"><![CDATA[SELECT DISTINCT party.Name, party.Postaladdress, party.PostalCode, party.DayPhone,
party.FaxPhone, SUBSTRING_INDEX(party.EmailAddress, ',', 1) AS EmailAddress, 
party.TaxNumber, event.ID, event.Date, CONCAT(event.Process, ' ', event.Name) AS Folio,
event.Notes, organization.Name AS OrgName, organization.Postaladdress AS OrgAddress, 
organization.PostalCode AS OrgPostalCode, organization.DayPhone AS OrgDayPhone, organization.FaxPhone AS OrgFax,
SUBSTRING_INDEX(organization.EmailAddress, ',', 1) AS OrgEmail, organization.TaxNumber AS OrgTaxNumber,
journal.Description, journal.Currency, EntityType, DebitAmount, CreditAmount, text.Data,
report.FromName, report.ToName, report.FromDate, report.ToDate
FROM report, journal
JOIN event ON event.ID = journal.EventID
JOIN party ON party.ID = journal.EntityID AND journal.EntityType = 'Party'
JOIN party AS organization ON organization.ID = journal.OrganizationID
JOIN text ON text.ID = CONCAT('Logo.', organization.ID, '.jpg')
WHERE report.ID = ?
AND party.Name >= report.FromName
AND party.Name <= report.ToName
AND event.Date <= report.ToDate
AND event.State not in ('Initial', 'Final')
AND journal.OrganizationID = report.OrganizationID
AND journal.Currency = report.Currency
ORDER BY party.Name, event.Date, journal.ID]]></xml-property>
