                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[SELECT account.Type, 
account.Name,
SUM(IF(event.Date < FromDate, 0.0, DebitAmount)) AS DebitAmount, 
SUM(IF(event.Date < FromDate, 0.0, CreditAmount)) AS CreditAmount,
SUM(DebitAmount - CreditAmount) AS ClosingAmount,
SUM(IF(event.Date < FromDate, DebitAmount-CreditAmount, 0.0)) AS OpeningAmount,
FromDate,
ToDate,
report.Currency
FROM report, journal
JOIN account ON account.ID = journal.AccountID
JOIN event ON event.ID = journal.EventID
JOIN party AS organization ON organization.ID = journal.OrganizationID
WHERE report.ID = ?
AND account.Type IN ('Capital', 'Current Asset', 'Current Liability', 'Fixed Asset', 'Long Term Liability', 'Other Asset')
AND event.Date <= report.ToDate
AND event.State not in ('Initial', 'Final')
AND journal.Currency = report.Currency
GROUP BY account.Name
ORDER BY account.Type, account.Name]]></xml-property>
        </oda-data-set>
