            <xml-property name="queryText"><![CDATA[SELECT party.Name AS Organization,
product.Name AS Product,
event.Date,
event.Process,
CONCAT(YEAR(event.Date),' ',MONTH(event.Date),' ',event.Process) AS Period,
DebitAmount,
CreditAmount,
journal.Currency
FROM report, journal
JOIN event ON journal.EventID = event.ID
JOIN product ON product.ID = journal.EntityID AND journal.EntityType = 'Product'
LEFT JOIN party ON party.ID = journal.OrganizationID
WHERE report.ID = ?
AND product.Name >= FromName
AND product.Name <= ToName
AND event.Date >= FromDate
AND event.Date <= ToDate 
AND event.State <> 'Final'
AND event.OrganizationID = report.OrganizationID
AND journal.Currency = report.Currency
]]></xml-property>
