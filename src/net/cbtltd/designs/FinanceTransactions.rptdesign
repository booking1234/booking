            <xml-property name="queryText"><![CDATA[SELECT CONCAT(event.Process, ' ', event.Name) as Folio, event.Date, event.Notes, 
DebitAmount, CreditAmount, DebitAmount - CreditAmount AS Amount, 
journal.Currency, 
journal.Description, 
finance.Name AS Finance
FROM report, journal
JOIN event ON journal.eventID = event.ID
JOIN finance ON finance.ID = journal.EntityID AND journal.EntityType = 'Finance'
WHERE report.ID = ?
AND finance.Name >= FromName 
AND finance.Name <= ToName 
AND event.Date >= FromDate
AND event.Date <= ToDate 
AND event.State NOT IN ('Initial', 'Final')
AND journal.OrganizationID = report.OrganizationID
AND journal.Currency = finance.Currency
AND (report.Process is null OR event.Process = report.Process)
AND (report.AccountID is null OR journal.AccountID = report.AccountID)
AND (report.EntityID is null OR journal.EntityID = report.EntityID)
ORDER BY finance.Name, event.Date]]></xml-property>
