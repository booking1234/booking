            <xml-property name="queryText"><![CDATA[SELECT DebitAmount, CreditAmount, DebitAmount - CreditAmount AS Amount,
reservation.Name AS Reservation,
organization.Name as Organization, 
party.Name AS Party, 
event.Date, event.ID, event.Process, event.Notes, 
report.FromName, report.ToName, report.FromDate, report.ToDate, report.ID
FROM report, reservation
JOIN event ON event.ParentID = reservation.ID AND event.ParentID IS NOT NULL
JOIN journal ON journal.EventID = event.ID
JOIN party AS organization ON organization.ID = journal.OrganizationID
JOIN party ON party.ID = journal.EntityID AND journal.Entitytype = 'Party' AND party.ID = reservation.CustomerID
WHERE report.ID = ?
AND reservation.Name >= report.FromName 
AND reservation.Name <= report.ToName
AND reservation.OrganizationID = report.OrganizationID
AND event.State not in ('Initial', 'Final')
ORDER BY reservation.Name, event.Date]]></xml-property>
