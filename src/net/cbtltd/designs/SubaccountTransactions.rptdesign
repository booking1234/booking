            <xml-property name="queryText"><![CDATA[SELECT organization.Name AS Organization,
account.Name AS Account,
event.Process,
event.Date,
CONCAT(event.Process, ' ', event.Name) AS Folio,
CONCAT(YEAR(event.Date), ' ', MONTH(event.Date), ' ', event.Process) AS Period,
DebitAmount, 
CreditAmount,
Description,
journal.Currency,
(CASE 
WHEN EntityType = 'Asset' THEN asset.Name
WHEN EntityType = 'Party' THEN party.Name
WHEN EntityType = 'Product' THEN product.Name 
WHEN EntityType = 'Finance' THEN finance.Name 
END) AS Subaccount
FROM report, journal 
JOIN event ON event.ID = journal.eventID
JOIN account ON account.ID = journal.AccountID
JOIN party AS organization ON organization.ID = journal.OrganizationID
LEFT JOIN asset on asset.ID = EntityID and Entitytype = 'Asset'
LEFT JOIN party on party.ID = EntityID and Entitytype = 'Party'
LEFT JOIN product on product.ID = EntityID and Entitytype = 'Product'
LEFT JOIN finance on finance.ID = EntityID and Entitytype = 'Finance'
WHERE report.ID = ?
AND account.Name >= FromName 
AND account.Name <= ToName
AND event.Date >= report.FromDate 
AND event.Date <= report.ToDate 
AND (report.Process is null OR event.Process = report.Process)
AND event.State not in ('Initial', 'Final')
AND journal.OrganizationID = report.OrganizationID
AND journal.Currency = report.Currency
ORDER BY account.Name, Subaccount, event.Date]]></xml-property>
        </oda-data-set>
